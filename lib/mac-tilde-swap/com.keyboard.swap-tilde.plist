<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>EnvironmentVariables</key>
    <dict>
     <key>PATH</key>
     <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:</string>
    </dict>
    <key>Label</key>
    <string>com.keyboard.swap-tilde</string>
    <key>Program</key>
    <string>/usr/local/bin/swap-tilde-on-keyboard.sh</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <false/>
    <key>LaunchOnlyOnce</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/startup.keyboard.swap-tilde.stdout</string>
    <key>StandardErrorPath</key>
    <string>/tmp/startup.keyboard.swap-tilde.stderr</string>
    <key>InitGroups</key>
    <true/>
  </dict>
</plist>
